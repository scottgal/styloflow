{
  "id": "sample-keyed-coordinator",
  "name": "Keyed Coordinator Pipeline",
  "description": "Coordinates work items by key, ensuring ordered processing per-key",
  "nodes": [
    {
      "id": "http-1",
      "manifestName": "http-receiver",
      "x": 100,
      "y": 200,
      "config": {}
    },
    {
      "id": "analyzer-1",
      "manifestName": "text-analyzer",
      "x": 350,
      "y": 200,
      "config": {}
    },
    {
      "id": "coordinator-1",
      "manifestName": "coordinator-keyed",
      "x": 600,
      "y": 200,
      "config": {
        "key_field": "user_id",
        "max_concurrent": 5
      }
    },
    {
      "id": "sentiment-1",
      "manifestName": "sentiment-detector",
      "x": 850,
      "y": 200,
      "config": {}
    },
    {
      "id": "reducer-1",
      "manifestName": "reduce-avg",
      "x": 1050,
      "y": 200,
      "config": {
        "window_name": "sentiment_scores"
      }
    },
    {
      "id": "log-1",
      "manifestName": "log-writer",
      "x": 1250,
      "y": 200,
      "config": {}
    }
  ],
  "edges": [
    {
      "id": "e1",
      "sourceNodeId": "http-1",
      "signalKey": "http.body",
      "targetNodeId": "analyzer-1"
    },
    {
      "id": "e2",
      "sourceNodeId": "analyzer-1",
      "signalKey": "text.analyzed",
      "targetNodeId": "coordinator-1"
    },
    {
      "id": "e3",
      "sourceNodeId": "coordinator-1",
      "signalKey": "coordinator.ready",
      "targetNodeId": "sentiment-1"
    },
    {
      "id": "e4",
      "sourceNodeId": "sentiment-1",
      "signalKey": "sentiment.score",
      "targetNodeId": "reducer-1"
    },
    {
      "id": "e5",
      "sourceNodeId": "reducer-1",
      "signalKey": "avg.result",
      "targetNodeId": "log-1"
    }
  ]
}
