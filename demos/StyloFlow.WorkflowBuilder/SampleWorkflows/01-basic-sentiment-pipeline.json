{
  "id": "sample-sentiment-pipeline",
  "name": "Basic Sentiment Pipeline",
  "description": "Timer triggers text analysis and sentiment detection, filters high-confidence results",
  "nodes": [
    {
      "id": "trigger-1",
      "manifestName": "timer-trigger",
      "x": 100,
      "y": 200,
      "config": {
        "interval_seconds": 5
      }
    },
    {
      "id": "analyzer-1",
      "manifestName": "text-analyzer",
      "x": 350,
      "y": 200,
      "config": {}
    },
    {
      "id": "sentiment-1",
      "manifestName": "sentiment-detector",
      "x": 600,
      "y": 200,
      "config": {}
    },
    {
      "id": "filter-1",
      "manifestName": "threshold-filter",
      "x": 850,
      "y": 150,
      "config": {
        "threshold": 0.7
      }
    },
    {
      "id": "log-1",
      "manifestName": "log-writer",
      "x": 1100,
      "y": 150,
      "config": {}
    }
  ],
  "edges": [
    {
      "id": "e1",
      "sourceNodeId": "trigger-1",
      "signalKey": "timer.triggered",
      "targetNodeId": "analyzer-1"
    },
    {
      "id": "e2",
      "sourceNodeId": "analyzer-1",
      "signalKey": "text.analyzed",
      "targetNodeId": "sentiment-1"
    },
    {
      "id": "e3",
      "sourceNodeId": "sentiment-1",
      "signalKey": "sentiment.score",
      "targetNodeId": "filter-1"
    },
    {
      "id": "e4",
      "sourceNodeId": "filter-1",
      "signalKey": "filter.passed",
      "targetNodeId": "log-1"
    }
  ]
}
